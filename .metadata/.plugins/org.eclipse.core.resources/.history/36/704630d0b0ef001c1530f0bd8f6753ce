package Recursions.KunalKushwahaYT;

public class ReverseOfNo {
	static int rev=1;
	static int reverse(int n) {
		if(n/10 == 0)
			return n;
		
		return (reverse(n/10)*10);		
	}

	public static void main(String[] args) {
//		System.out.println(reverse(1824));
		int r = 0;
		r = r*10 + 4;
		System.out.println(r);
		
	}

}
