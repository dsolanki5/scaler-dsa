package BinarySearch;

public class SquareRoot {
	
	public static int sqrtN(long N) {
		int s = 2;
        int e = (int)(N/2);
        int mid = s + (e-s)/2;
        
        while(s<e) {
            if((mid*mid) >= N) {
                e = mid;
            }
            else {
                s = mid+1;
            }
            mid = s + (e-s)/2;
        }
        return e;
	}

	public static void main(String[] args) {
		
		System.out.println(sqrtN(25));

	}

}
