package Recursions.KunalKushwahaYT;

public class ReverseOfNo {
	static int rev=0;
	static int reverse(int n) {
		if(n%10 == n)
			return n;
		
		return n%10+reverse(n/10)*10;		
	}

	public static void main(String[] args) {
		System.out.println(reverse(1823));
//		int r = 0;
//		r = r*10 + 4;
//		System.out.println(r);
		
	}

}
