package Recursions.KunalKushwahaYT;

public class ReverseOfNo {
	static int rev=0;
	static int reverse(int n) {
		if(n/10 == 0)
			return rev*10+n;
		rev = (rev*10) + (n%10);
		
		return reverse(n/10);		
	}

	public static void main(String[] args) {
		System.out.println(reverse(5));
//		int r = 0;
//		r = r*10 + 4;
//		System.out.println(r);
		
	}

}
