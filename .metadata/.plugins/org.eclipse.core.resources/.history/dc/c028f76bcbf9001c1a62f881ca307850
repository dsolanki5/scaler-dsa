package Recursions.KunalKushwahaYT.subsetsubsequences;

public class SkipCharInString {
	
	public static String skpCharInStr(String ip, String op) {
		if(ip.length() == 0)
			return op;
		
		char ch = ip.charAt(0);
		if(ch == 'a' || ch == 'A') {
			return skpCharInStr(ip.substring(1), op);
		}
		else {
			return skpCharInStr(ip.substring(1), ch+op);
		}
	}

	public static void main(String[] args) {
		
		System.out.println(skpCharInStr("abdacd", ""));

	}

}
